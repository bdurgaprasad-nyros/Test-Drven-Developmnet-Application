<div class="span-15" style="width:624px; padding-top:8px;padding-bottom:6px;margin-left:6px;">
     

	<% unless (controller.action_name=="account_settings" or controller.action_name=="account_update" ) %>

	    <div class="span-3" style="float:left;">
				<%= link_to image_tag("sprint3/profiles/profiletab_btn.png"), {:controller=>'profile',:action=>'basic_information',
			   :id=>"#{current_user.id}"} if user_signed_in? %>
		</div>
		<div class="span-4" style="float:left;" >
			<%= link_to image_tag("sprint3/profiles/accountinactive_btn.png"), profile_account_settings_path %>

		</div>
		
	
	<% else %>
	    <div class="span-3" style="float:left;">


			<%= link_to image_tag("sprint3/profiles/profileinactive_btn.png"),  {:controller=>'profile',:action=>'basic_information',
		   	:id=>"#{current_user.id}"}  if user_signed_in?%> 
		</div>
		<div class="span-4" style="float:left;" >
			<%= link_to image_tag("sprint3/profiles/accounttab_btn.png"), profile_account_settings_path %> 
		</div>
	<% end %> 
	
</div>	
    
	