<%= form_for @notification_subscription do |f| %>
  <%#= f.error_messages %>
  <p>
    <%= f.label :group_id %><br />
    <%= f.text_field :group_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :notification_type_id %><br />
    <%= f.text_field :notification_type_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
